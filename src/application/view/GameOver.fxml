      <MediaView fx:id="bgMV" fitHeight="600.0" fitWidth="700.0" />
      <ImageView fitHeight="600.0" fitWidth="700.0" visible="false">
         <image>
            <Image url="@../../../assets/gameOver/resized.gif" />
         </image>
      </ImageView>
      <ImageView fitHeight="53.0" fitWidth="80.0" layoutX="310.0" layoutY="410.0" onMouseClicked="#toMainMenu">
         <image>
            <Image url="@../../../assets/backgrounds/endbutton.png" />
         </image>
      </ImageView>
      <VBox alignment="CENTER" layoutX="276.0" layoutY="279.0" prefHeight="118.0" prefWidth="148.0">
         <children>
            <Label fx:id="scoreLabel" alignment="CENTER" contentDisplay="CENTER" text="100" textAlignment="CENTER" textFill="#fffbfb">
               <font>
                  <Font name="System Bold" size="72.0" />
               </font>
            </Label>
         </children>
      </VBox>
   </children>
